<% if(errors.length > 0) { %>
<div class="row my-3">
    <div class="col my-3">
      <div class="alert alert-danger">
          <% errors.forEach(erro => { %>
            <%= erro %> <br>
          <% }); %>
      </div>
    </div>
  </div>
<% } %>


<% if(success.length > 0) { %>
    <div class="row my-3">
        <div class="col my-3">
          <div class="alert alert-success">
              <% success.forEach(successo => { %>
                <%= successo %> <br>
              <% }); %>
          </div>
        </div>
      </div>
 
    <% } %>